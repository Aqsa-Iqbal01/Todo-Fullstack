{
  "version": 2,
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "includeFiles": ["requirements.txt"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "src/main.py"
    }
  ],
  "functions": {
    "src/main.py": {
      "maxDuration": 60
    }
  }
}